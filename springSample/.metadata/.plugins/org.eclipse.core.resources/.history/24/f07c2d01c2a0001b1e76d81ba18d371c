<!-- 매핑된 SQL 구문 살펴보기 -->
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="Member">
	<select id="idcheck" parameterType="String" resultType="Integer">
 		SELECT COUNT(*) FROM MEMBER WHERE ID=#{id}
 	</select>
 </mapper> 
 
 